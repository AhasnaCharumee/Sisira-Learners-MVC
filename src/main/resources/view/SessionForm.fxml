<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="paneSession" prefHeight="612.0" prefWidth="1172.0" stylesheets="@../CSS/styles.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.gdse72.sisiralearners.controller.SessionFormController">
   <children>
      <BorderPane layoutX="3.0" layoutY="3.0" prefHeight="612.0" prefWidth="1172.0">
         <top>
            <Pane prefHeight="75.0" prefWidth="1172.0" BorderPane.alignment="CENTER">
               <children>
                  <Label layoutX="35.0" layoutY="27.0" prefHeight="24.0" prefWidth="205.0" text="Session Management">
                     <font>
                        <Font name="System Bold" size="17.0" />
                     </font>
                  </Label>
                  <Button fx:id="btnSave" layoutX="996.0" layoutY="19.0" mnemonicParsing="false" onAction="#btnSaveOnAction" prefHeight="38.0" prefWidth="141.0" text="ADD NEW">
                     <graphic>
                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../assets/icons/icons8-add-60.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </Pane>
         </top>
         <center>
            <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <TableView fx:id="tblSession" layoutX="38.0" layoutY="130.0" prefHeight="362.0" prefWidth="1097.0">
                     <columns>
                        <TableColumn fx:id="colSessionId" prefWidth="122.0" text="SESSION ID" />
                        <TableColumn fx:id="colInstructor" prefWidth="221.0" text="INSTRUCTOR NAME" />
                        <TableColumn fx:id="colVehicle" minWidth="0.0" prefWidth="217.0" text="VEHICLE NAME" />
                        <TableColumn fx:id="colDay" minWidth="0.0" prefWidth="200.0" text="DAY" />
                        <TableColumn fx:id="colStartTime" minWidth="0.0" prefWidth="166.0" text="START TIME" />
                        <TableColumn fx:id="colEnadTime" prefWidth="170.0" text="END TIME" />
                     </columns>
                  </TableView>
                  <JFXComboBox fx:id="cmbDay" labelFloat="true" layoutX="522.0" layoutY="28.0" onAction="#cmbDayOnAction" prefHeight="26.0" prefWidth="130.0" promptText="Day" />
                  <JFXTextField fx:id="txtSessionId" layoutX="38.0" layoutY="28.0" prefHeight="26.0" prefWidth="94.0" promptText="Session ID" />
                  <JFXTextField fx:id="txtStartTime" layoutX="690.0" layoutY="28.0" prefHeight="26.0" prefWidth="131.0" promptText="Engine Number" text="Start Time" />
                  <Button fx:id="btnDelete" layoutX="890.0" layoutY="82.0" mnemonicParsing="false" onAction="#btnDeleteOnAction" prefHeight="34.0" prefWidth="102.0" text="Delete" />
                  <Button fx:id="btnUpdate" layoutX="1023.0" layoutY="82.0" mnemonicParsing="false" onAction="#btnUpadateOnAction" prefHeight="34.0" prefWidth="111.0" text="Update" />
                  <JFXTextField fx:id="txtEndTime" layoutX="861.0" layoutY="28.0" prefHeight="26.0" prefWidth="128.0" promptText="Engine Number" text="End Time" />
                  <JFXComboBox fx:id="cmbInstructorNames" layoutX="176.0" layoutY="28.0" prefHeight="26.0" prefWidth="128.0" promptText="Instructor" />
                  <JFXComboBox fx:id="cmbVehicleName" layoutX="337.0" layoutY="28.0" prefHeight="26.0" prefWidth="128.0" promptText="Vehicle" />
                  <ImageView fx:id="btnReset" fitHeight="42.0" fitWidth="44.0" layoutX="1017.0" layoutY="20.0" onMouseClicked="#btnResetOnAction" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../assets/icons/icons8-reset-90.png" />
                     </image>
                  </ImageView>
               </children>
            </Pane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
